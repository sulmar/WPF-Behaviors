            <Rectangle Fill="#FF3535E5" Height="51.242" Stroke="Black" Width="115.685" Margin="0,0,20,20"/>
            <Rectangle Fill="#FF3535E5" Height="51.242" Stroke="Black" Width="115.685" Margin="0,0,20,20"/>
            <Rectangle Fill="#FF3535E5" Height="51.242" Stroke="Black" Width="115.685" Margin="0,0,20,20"/>
            <Rectangle Fill="#FF3535E5" Height="51.242" Stroke="Black" Width="115.685" Margin="0,0,20,20"/>
            <Rectangle Fill="#FF3535E5" Height="51.242" Stroke="Black" Width="115.685" Margin="0,0,20,20"/>
            <Rectangle Fill="#FF3535E5" Height="51.242" Stroke="Black" Width="115.685" Margin="0,0,20,20"/>
            <Rectangle Fill="#FF3535E5" Height="51.242" Stroke="Black" Width="115.685" Margin="0,0,20,20"/>
            <Rectangle Fill="#FF3535E5" Height="51.242" Stroke="Black" Width="115.685" Margin="0,0,20,20"/>
        </WrapPanel>

    </Grid>
